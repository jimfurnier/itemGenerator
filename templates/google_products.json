{
  "output_name": "google",
  "compression": "gzip",
  "delimiter": "comma",
  "columns": [
    {"name": "id", "type": "rowNumber"},
    {"name": "title", "type": "product"},
    {"name": "description", "type": "description"},
    {"name": "google_product_category", "type": "category"},
    {"name": "product_type", "type": "category"},
    {"name": "link", "type": "url"},
    {"name": "mobile_link", "type": "url"},
    {"name": "image_link", "type": "image"},
    {"name": "additional_image_link", "type": "image"},
    {"name": "condition", "type": "string"},
    {"name": "availability", "type": "random", "random":  ["in_stock", "out_stock"]},
    {"name": "price", "type": "float"},
    {"name": "sale_price", "type": "float"},
    {"name": "tax", "type": "percent"},
    {"name": "shipping", "type": "float"},
    {"name": "shipping_weight", "type": "integer"},
    {"name": "sale_price_effective_date", "type": "datetime"},
    {"name": "brand", "type": "string"},
    {"name": "gtin", "type": "uuid"},
    {"name": "mpn", "type": "uuid", "optional":  50},
    {"name": "gender", "type": "gender"},
    {"name": "age_group", "type": "random", "random": ["young", "teens", "old"]},
    {"name": "color", "type": "color"},
    {"name": "size", "type": "string"},
    {"name": "size_type", "type": "string"},
    {"name": "size_system", "type": "string"},
    {"name": "material", "type": "material"},
    {"name": "pattern", "type": "string"},
    {"name": "item_group_id", "type": "string"},
    {"name": "adwords_grouping", "type": "string"},
    {"name": "adwords_labels", "type": "string"},
    {"name": "adwords_redirect", "type": "url"},
    {"name": "promotion_id", "type": "uuid"},
    {"name": "display_ads_title", "type": "string"},
    {"name": "custom_label_0", "type": "string", "optional":  20},
    {"name": "custom_label_1", "type": "string", "optional":  20},
    {"name": "custom_label_2", "type": "string", "optional":  20},
    {"name": "custom_label_3", "type": "string", "optional":  20},
    {"name": "custom_label_4", "type": "string", "optional":  20}
  ]
}
